Bundle-Name: Xtivia Service Guard DXP Sample
Bundle-SymbolicName: com.xtivia.sample.sgdxp
Bundle-Version: 1.9.0
Require-Capability:\
	osgi.contract;\
		filter:="(&(osgi.contract=JavaJAXRS)(version=2))"
Import-Package: \
	*
Bundle-ClassPath:\
	.,\
	lib/jackson-jaxrs-json-provider.jar,\
	lib/jackson-jaxrs-base.jar,\
	lib/jackson-module-jaxb-annotations.jar,\
	lib/javax.activation-api.jar
-includeresource:\
	lib/jackson-jaxrs-json-provider.jar=jackson-jaxrs-json-provider-*.jar,\
	lib/jackson-jaxrs-base.jar=jackson-jaxrs-base-*.jar,\
	lib/jackson-module-jaxb-annotations.jar=jackson-module-jaxb-annotations-*.jar,\
	lib/javax.activation-api.jar=javax.activation-api-*.jar
